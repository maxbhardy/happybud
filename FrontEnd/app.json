{
  "expo": {
    "name": "HappyBud",
    "slug": "Happybud",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/happybud_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/happybud_adaptive.png",
        "backgroundColor": "#DFD8D1"
      },
      "package": "com.happybud.HappyBud"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/happybud_splash.png",
          "imageWidth": 400,
          "resizeMode": "contain",
          "backgroundColor": "#DFD8D1"
        }
      ],
      "expo-asset",
      "onnxruntime-react-native",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
